<?xml version="1.0" encoding="utf-8"?>
<xs:schema
	xmlns:xs="http://www.w3.org/2001/XMLSchema"
	xmlns:in="https://incomaker.com/feed/2.15"
>
	<xs:import namespace="https://incomaker.com/feed/2.15" schemaLocation="incomaker.xsd"/>
	<xs:element name="orders">
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="in:feed">
					<xs:sequence>
						<xs:element name="o" minOccurs="0" maxOccurs="unbounded">
							<xs:complexType>
								<xs:all>
									<xs:element name="contactId" type="xs:string" minOccurs="0" />
									<xs:element name="contact" type="in:contact" minOccurs="0"/>
									<xs:element name="created" type="in:dateTime" />
									<xs:element name="finished" type="in:dateTime" minOccurs="0" />
									<xs:element name="payment" type="xs:string" minOccurs="0" />
									<xs:element name="state" type="xs:string" minOccurs="0" />
									<xs:element name="total" type="xs:decimal" minOccurs="0" />
									<xs:element name="totalWithoutTax" type="xs:decimal" minOccurs="0" />
									<xs:element name="currency" type="xs:string" minOccurs="0" />
									<xs:element name="customFields" type="in:groupedCustomFields" minOccurs="0" />
									<xs:element name="items">
										<xs:complexType>
											<xs:sequence>
												<xs:element name="i" minOccurs="0" maxOccurs="unbounded">
													<xs:complexType>
														<xs:all>
															<xs:element name="variantId" type="xs:string" minOccurs="0" />
															<xs:element name="quantity" type="xs:decimal"/>
															<xs:element name="price">
																<xs:complexType>
																	<xs:simpleContent>
																		<xs:extension base="xs:decimal">
																			<xs:attribute name="currency" type="xs:string"/>
																		</xs:extension>
																	</xs:simpleContent>
																</xs:complexType>
															</xs:element>
															<xs:element name="priceWithoutTax" minOccurs="0" >
																<xs:complexType>
																	<xs:simpleContent>
																		<xs:extension base="xs:decimal">
																			<xs:attribute name="currency" type="xs:string"/>
																		</xs:extension>
																	</xs:simpleContent>
																</xs:complexType>
															</xs:element>
															<xs:element name="currency" type="xs:string" minOccurs="0" />
														</xs:all>
														<xs:attribute name="id" type="xs:string"/>
													</xs:complexType>
												</xs:element>
											</xs:sequence>
										</xs:complexType>
									</xs:element>
								</xs:all>
								<xs:attribute name="id" type="xs:string"/>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>

</xs:schema>
